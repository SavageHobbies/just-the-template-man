    "@typescript-eslint/parser": "^6.21.0",
    "eslint": "^8.0.0",
    "rimraf": "^5.0.0",
    "ts-node": "^10.0.0",
    "typescript": "^5.0.0",
    "vitest": "^1.0.0"
  },
  "dependencies": {
    "axios": "^1.6.0",
    "chalk": "^5.5.0",
    "cheerio": "^1.0.0-rc.12",
