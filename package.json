{
  "name": "ebay-listing-optimizer",
  "version": "1.0.0",
  "description": "Automated eBay listing optimization tool",
  "main": "dist/index.js",
  "bin": {
    "ebay-optimizer": "dist/cli.js"
  },
  "scripts": {
    "build": "tsc",
    "dev": "ts-node src/index.ts",
    "cli": "ts-node src/cli.ts",
    "test": "vitest",
    "test:run": "vitest run",
    "test:e2e": "node scripts/end-to-end-test.js",
    "lint": "eslint src/**/*.ts",
